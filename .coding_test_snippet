ct(){
    echo 'import sys' > ct.py
    echo '' >> ct.py
    echo 'input = sys.stdin.readline' >> ct.py
    echo '' >> ct.py
    echo '' >> ct.py
    echo 'def iinput():' >> ct.py
    echo '    return int(input().strip())' >> ct.py
    echo '' >> ct.py
    echo '' >> ct.py
    echo 'def lstinput():' >> ct.py
    echo '    return list(input().strip())' >> ct.py
    echo '' >> ct.py
    echo '' >> ct.py
    echo 'def lstinput_map_int():' >> ct.py
    echo '    return list(map(int, list(input().strip())))' >> ct.py
    echo '' >> ct.py
    echo '' >> ct.py
    echo 'def lstinput():' >> ct.py
    echo '    return list(input().strip())' >> ct.py
    echo '' >> ct.py
    echo '' >> ct.py
    echo 'def spt(sep=" "):' >> ct.py
    echo '    return input().strip().split(sep)' >> ct.py
    echo '' >> ct.py
    echo '' >> ct.py
    echo 'def spt_map_int(sep=" "):' >> ct.py
    echo '    return list(map(int, input().strip().split(sep)))' >> ct.py
    echo '' >> ct.py
    echo '' >> ct.py
}

rct(){
    python ct.py
}