    echo '    return list(map(int, input().strip().split(sep)))' >> codingTest.py
    echo '' >> codingTest.py
    echo '' >> codingTest.py
    echo 'def sol():' >> codingTest.py
    echo '    'pass >> codingTest.py
    echo '' >> codingTest.py
