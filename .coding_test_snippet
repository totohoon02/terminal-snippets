    echo 'def lstinput():' >> codingTest.py
    echo '    return list(input().strip())' >> codingTest.py
    echo '' >> codingTest.py
    echo '' >> codingTest.py
    echo 'def lstinput_map_int():' >> codingTest.py
    echo '    return list(map(int, list(input().strip())))' >> codingTest.py
    echo '' >> codingTest.py
    echo '' >> codingTest.py
    echo 'def lstinput():' >> codingTest.py
    echo '    return list(input().strip())' >> codingTest.py
    echo '' >> codingTest.py
    echo '' >> codingTest.py
    echo 'def spt(sep=" "):' >> codingTest.py
    echo '    return input().strip().split(sep)' >> codingTest.py
    echo '' >> codingTest.py
    echo '' >> codingTest.py
    echo 'def spt_map_int(sep=" "):' >> codingTest.py
    echo '    return list(map(int, input().strip().split(sep)))' >> codingTest.py
    echo '' >> codingTest.py
    echo '' >> codingTest.py
}

